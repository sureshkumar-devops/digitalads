<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.2.0 http://maven.apache.org/xsd/settings-1.2.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.2.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <username>jenkins</username>
      <password>Admin@123</password>
      <id>release</id>
    </server>
    <server>
      <username>jenkins</username>
      <password>Admin@123</password>
      <id>snapshots</id>
    </server>
      <server>
      <username>jenkins</username>
      <password>Admin@123</password>
      <id>digitalads-group-virtual</id>
    </server>
  </servers>
  <mirrors>
    <mirror>
      <mirrorOf>*</mirrorOf>
      <name>digitalads-group-virtual</name>
      <url>http://localhost:8081/artifactory/digitalads-group-virtual</url>
      <id>digitalads-group-virtual</id>
    </mirror>
  </mirrors>
  <profiles>
    <profile>
      <repositories>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>release</id>
          <name>digitalads-local-release</name>
          <url>http://localhost:8081/artifactory/digitalads-local-release</url>
        </repository>
        <repository>
          <snapshots />
          <id>snapshots</id>
          <name>digitalads-local-snapshot</name>
          <url>http://localhost:8081/artifactory/digitalads-local-snapshot</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>release</id>
          <name>digitalads-local-release</name>
          <url>http://localhost:8081/artifactory/digitalads-local-release</url>
        </pluginRepository>
        <pluginRepository>
          <snapshots />
          <id>snapshots</id>
          <name>digitalads-local-snapshot</name>
          <url>http://localhost:8081/artifactory/digitalads-local-snapshot</url>
        </pluginRepository>
      </pluginRepositories>
      <id>artifactory</id>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>artifactory</activeProfile>
  </activeProfiles>
</settings>
